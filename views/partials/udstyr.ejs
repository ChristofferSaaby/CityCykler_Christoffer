        
        <li id="produkter">
            <%
                    
            udstyr.forEach( (ar)=> { %>
                <div id="udstyr-info">
                    <%- `<h1>${ar.mærke} ${ar.model}</h1>` %>
                        <%- `<p><img src="/img/${ar.billede}"></p>` %>
                            <%- `<p>${ar.beskrivelse}</p>` %>
                                 <%- `<p><strong>Pris:</strong> ${ar.pris} kr.</p>` %>
                                 <%- `<a class="læsMere" href="">Læs Mere</a>` %>
                                    <%- `<br>` %>
                </div>
                <% }); %>
        </li>
    <!-- cykel-kategori end -->